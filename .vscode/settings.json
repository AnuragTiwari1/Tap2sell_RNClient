{
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "editor.formatOnSave": true,
  "cSpell.words": [
    "Denk",
    "Lato",
    "Walkthroughable",
    "Xiaomi",
    "antdesign",
    "entypo",
    "errored",
    "wifi"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
}